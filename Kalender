package projekt2sem;

import java.util.ArrayList;

/**
 *
 * @author Kasper
 */
public class Kalender {
    
    public ArrayList maaned(){
        ArrayList<Integer> m = new ArrayList();
        for(int i = 1; i < 13; i++){
            m.add(i);
        }
        return m;
    }
    public ArrayList dage(){
        ArrayList<Integer> d = new ArrayList();
        for(int i = 1; i < 32; i++){
            d.add(i);
        }
        return d;
    }
    public ArrayList aar(){
        ArrayList<Integer> aar = new ArrayList();
        for(int i = 0; i < 16; i++){
            int years = 2014;
            years += i;
            aar.add(years);
        }
        return aar;
    }
}
