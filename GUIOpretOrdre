    private void jButtonStartdatoActionPerformed(java.awt.event.ActionEvent evt) {                                                 
        // TODO add your handling code here:
        if(jListDag.isSelectionEmpty() || jListMaaned.isSelectionEmpty() || jListAar.isSelectionEmpty()){
           JOptionPane.showMessageDialog(null, "Dato eksisterer ikke!"); 
        }
        else if(con.dateError(jListDag.getSelectedValue(), jListMaaned.getSelectedValue(), jListAar.getSelectedValue())){
            JOptionPane.showMessageDialog(null, "Dato eksisterer ikke!");
        }
        else{
        jLabelStartDato.setText((int)jListDag.getSelectedValue() + "-" + (int)jListMaaned.getSelectedValue() + "-" + (int)jListAar.getSelectedValue());
        }
    }                                                

    private void jButtonSlutdatoActionPerformed(java.awt.event.ActionEvent evt) {                                                
        // TODO add your handling code here:
        if(jListDag.isSelectionEmpty() || jListMaaned.isSelectionEmpty() || jListAar.isSelectionEmpty()){
           JOptionPane.showMessageDialog(null, "Dato eksisterer ikke!"); 
        }
        else if(con.dateError(jListDag.getSelectedValue(), jListMaaned.getSelectedValue(), jListAar.getSelectedValue())){
            JOptionPane.showMessageDialog(null, "Dato eksisterer ikke!");
        }
        else{
        jLabelSlutDato.setText((int)jListDag.getSelectedValue() + "-" + (int)jListMaaned.getSelectedValue() + "-" + (int)jListAar.getSelectedValue());
        }
    }                                               

